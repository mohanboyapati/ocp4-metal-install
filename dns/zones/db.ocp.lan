$TTL    604800
@       IN      SOA     okd-svc.okd.lan. contact.okd.lan (
                  1     ; Serial
             604800     ; Refresh
              86400     ; Retry
            2419200     ; Expire
             604800     ; Minimum
)
        IN      NS      okd-svc

okd-svc.okd.lan.          IN      A       192.168.100.1

; Temp Bootstrap Node
okd-bootstrap.lab.okd.lan.        IN      A      192.168.100.200

; Control Plane Nodes
okd-cp-1.lab.okd.lan.         IN      A      192.168.100.201
okd-cp-2.lab.okd.lan.         IN      A      192.168.100.202
okd-cp-3.lab.okd.lan.         IN      A      192.168.100.203

; Worker Nodes
okd-w-1.lab.okd.lan.        IN      A      192.168.100.211
okd-w-2.lab.okd.lan.        IN      A      192.168.100.212
okd-w-3.lab.okd.lan.        IN      A      192.168.100.213


; OpenShift Internal - Load balancer
api.lab.okd.lan.        IN    A    192.168.100.1
api-int.lab.okd.lan.    IN    A    192.168.100.1
*.apps.lab.okd.lan.     IN    A    192.168.100.1

; ETCD Cluster
etcd-0.lab.okd.lan.    IN    A     192.168.100.201
etcd-1.lab.okd.lan.    IN    A     192.168.100.202
etcd-2.lab.okd.lan.    IN    A     192.168.100.203

; OpenShift Internal SRV records (cluster name = lab)
_etcd-server-ssl._tcp.lab.okd.lan.    86400     IN    SRV     0    10    2380    etcd-0.lab
_etcd-server-ssl._tcp.lab.okd.lan.    86400     IN    SRV     0    10    2380    etcd-1.lab
_etcd-server-ssl._tcp.lab.okd.lan.    86400     IN    SRV     0    10    2380    etcd-2.lab

oauth-openshift.apps.lab.okd.lan.     IN     A     192.168.100.1
console-openshift-console.apps.lab.okd.lan.     IN     A     192.168.100.1
